{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": {
          "type": "file",
          "path": "./android/app/google-services.json"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": {
          "type": "file",
          "path": "./android/app/google-services.json"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": {
          "type": "file",
          "path": "./android/app/google-services.json"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
