{
  "expo": {
    "name": "Silver",
    "slug": "silverapp",
    "version": "1.0.0",
    "scheme": "silver-pos",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/silvers.png",
    "splash": {
      "image": "./assets/silvers.png",
      "resizeMode": "contain",
      "backgroundColor": "#378388"
    },
    "platforms": [
      "android"
    ],
    "android": {
      "package": "com.silver.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/silvers.png",
        "backgroundColor": "#378388"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "notification": {
      "icon": "./assets/silvers.png",
      "color": "#378388",
      "androidMode": "default",
      "androidCollapsedTitle": "New notification"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": false,
          "root": "./src/app"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your products.",
          "cameraPermission": "The app accesses your camera to let you take photos for your products."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false,
        "root": "./src/app"
      },
      "eas": {
        "projectId": "6a7a08ba-8bdc-42a9-aa27-b72531749306"
      }
    },
    "owner": "tarsilversapp"
  }
}
